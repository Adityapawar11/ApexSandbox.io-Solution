public Integer nextPrime(Integer num) {
    //code here
    
   if(isPrime(num++) == true){
       return num++;
   }
}
 public Boolean isPrime(Integer n){
     if( n <= 1){
         return false ;
     }
     else{
         for(Integer i = 2 ; i < n-1 ; i++){
             if(Math.mod(n,i) == 0){
                 return false;
             }
             else true;
         }
     }
 }