public Integer nextPrime(Integer num) {
    //code here
    Boolean isItPrime = true;
    Integer prime = isPrime(num );
    if(prime == 0){
        isPrime(num++);
    }
    else 
        return prime;
    
    
}
public Integer isPrime(Integer n){
    for(Integer i = 2 ; i < n-1 ; i++){
        if(Math.mod(n,i) != 0){
            return n;
        }
        return 0;
    }
}