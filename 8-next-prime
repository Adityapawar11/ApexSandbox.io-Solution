public Integer nextPrime(Integer num) {
    //code here
    Boolean flag = false;
   Integer prime = num+1;
   isPrime(prime);
   if(flag )
    return prime;
    else
        prime++;
}
 public void isPrime(Integer n){
     if( n <= 1){
         flag = false ;
     }
     else{
         for(Integer i = 2 ; i < n-1 ; i++){
             if(Math.mod(n,i) != 0){
                 flag = true;
             }
             flag = false;
         }
     }
 }