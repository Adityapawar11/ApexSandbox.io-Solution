public Integer nextPrime(Integer num) {
    //code here
   if(num < 2) {
       return 2;
   }
   Boolean isPrime  = true;
   num ++;
   while(true){
   for(Integer i = 2; i < Math.sqrt(num); i++){
        if(Math.mod(num,i) == 0){
            isPrime = false;
            break;
        }
    }
    if(isPrime){
        return num;
    }else{
        num++;
    }
   }
 }