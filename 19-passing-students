public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    //code here
    if(passedExam == true && passedAssignment == true){
            return true;
        }else if(passedAssignment ==  true && passedProject == true){
            return true;
        }else if(passedExam == true && passedProject == true){
            return true;
        }else{
            return false;
        }
}
