public Integer fibonacci(Integer n) {
    //code here
    Integer current ;
    Integer previous = 1;
    Integer beforePrevious = 1 ;
    Integer count = 0;
    while(count < n-2){
        if( n == 1) {
            return previous ;
            
        }else if( n == 2){
            return previous;
        }else {
            current = previous + beforePrevious;
            beforePrevious = previous;
            previous = current ;
            count++;
        }

        
    }
    return current;
}