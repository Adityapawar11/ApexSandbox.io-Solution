public Id insertCourse(String name, String details, Integer credits) {
    // code here
    if(credits <= 4){
        apxio__Course__c apx = new apxio__Course__c();
        apx.Name = name;
        apx.apxio__Course_Details__c = details;
        apx.apxio__Credits__c = String.valueof(credits);
        insert apx;
    }
    return apx.Id;
}