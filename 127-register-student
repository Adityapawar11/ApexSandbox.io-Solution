public String registerStudent(String name, String phone, String email) {
    // code here
    apxio__Student__c apx = new apxio__Student__c();
    apx.apxio__Phone__c = phone;
    apx.apxio__Email__c = email;
    apx.Name = name;
    insert apx;
    List<apxio__Student__c> student = [Select apxio__Registration_Number__c from apxio__Student__c] ;
    for(apxio__Student__c stud : student ){
        if(stud.Id == apx.Id){
            return stud.apxio__Registration_Number__c
        }
    }
    return null;
}